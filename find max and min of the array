#include <iostream>
#include <climits>
using namespace std;



int main() {

int arr[100];

int num;




cout << "Enter the Size of Array : ";

cin >> num;
    

cout << "Enter the Elements of Array : ";

for(int i=0; i<num; i++){
        cin >> arr[i];
    }



int max_no = INT_MIN;

int min_no = INT_MAX;
    



    for(int i = 0; i < num; i++){
        max_no = max(max_no, arr[i]);
        min_no = min(min_no, arr[i]);

    cout << "Max = " << max_no << " Min = " << min_no;

    return 0;
}
